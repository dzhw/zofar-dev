<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	<component>
		<component-type>UIXmlEditor</component-type>
		<component-class>de.dzhw.manager.ui.components.UIXmlEditor</component-class>
	</component>
	
	<validator>
	<validator-id>ImportValidator</validator-id>	
	<validator-class>de.dzhw.manager.ui.validator.ImportValidator</validator-class>
</validator>
</faces-config>
